@PACKAGE_INIT@
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@-export.cmake")
check_required_components("@PROJECT_NAME@")

set_and_check(convert_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_INCLUDEDIR@")
set(convert_LIBRARIES "@PROJECT_NAME@")
set(convert_DEFINITIONS "@convert_DEFINITIONS@")

# Find required packages, ensuring that dynamically linked libraries will be found
if ("CONVERT_EIGEN" IN_LIST convert_DEFINITIONS)
    find_package(Eigen3 @MIN_EIGEN_VERSION@ REQUIRED)
endif()
if ("CONVERT_GTSAM" IN_LIST convert_DEFINITIONS)
    find_package(GTSAM REQUIRED)
endif()
if ("CONVERT_MANIF" IN_LIST convert_DEFINITIONS)
    find_package(manif REQUIRED)
endif()
if ("CONVERT_MATLAB" IN_LIST convert_DEFINITIONS)
    find_package(Matlab REQUIRED MEX_COMPILER)
endif()
if ("CONVERT_ROS" IN_LIST convert_DEFINITIONS)
    find_package(geometry_msgs REQUIRED)
    find_package(nav_msgs REQUIRED)
    find_package(sensor_msgs REQUIRED)
    find_package(std_msgs REQUIRED)
endif()
if ("CONVERT_SENSORBOX" IN_LIST convert_DEFINITIONS)
    find_package(sensorbox REQUIRED)
endif()
